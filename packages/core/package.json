{
  "name": "expo-gaode-map",
  "version": "2.2.2",
  "description": "一个功能完整的高德地图 React Native 组件库，基于 Expo Modules 开发，提供地图显示、定位、覆盖物等功能。",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "android",
    "ios",
    "app.plugin.js",
    "expo-module.config.json",
    "plugin/build"
  ],
  "scripts": {
    "build": "expo-module build && pnpm run build:plugin",
    "build:plugin": "tsc --project plugin/tsconfig.json",
    "clean": "expo-module clean && rm -rf plugin/build",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "expo-module prepare && pnpm run build:plugin",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "xed example/ios",
    "open:android": "open -a \"Android Studio\" example/android",
    "publish:next": "pnpm publish --tag next",
    "publish:latest": "pnpm publish --tag latest"
  },
  "keywords": [
    "react-native",
    "expo",
    "expo-gaode-map",
    "ExpoGaodeMap",
    "高德地图"
  ],
  "repository": "https://github.com/TomWq/expo-gaode-map",
  "bugs": {
    "url": "https://github.com/TomWq/expo-gaode-map/issues"
  },
  "author": "尚博信_王强 <wangqiang03@sunboxsoft.com> (https://github.com/TomWq)",
  "license": "MIT",
  "homepage": "https://github.com/TomWq/expo-gaode-map#readme",
  "dependencies": {
    "supercluster": "^8.0.1"
  },
  "devDependencies": {
    "@expo/config-plugins": "^9.1.7",
    "@types/react": "~19.1.0",
    "expo": "^54.0.18",
    "expo-module-scripts": "^5.0.7",
    "react-native": "0.81.5",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  }
}
