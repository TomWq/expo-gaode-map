{
  "name": "expo-gaode-map-monorepo",
  "version": "1.0.1",
  "private": false,
  "description": "高德地图 Expo 模块 Monorepo",
  "scripts": {
    "build": "pnpm -r --filter './packages/*' run build",
    "clean": "pnpm -r --filter './packages/*' run clean",
    "lint": "pnpm -r --filter './packages/*' run lint",
    "test": "pnpm -r --filter './packages/*' run test",
    "publish:core": "cd packages/core && pnpm publish --access public",
    "publish:search": "cd packages/search && pnpm publish --access public",
    "publish:navigation": "cd packages/navigation && pnpm publish --access public",
    "publish:web-api": "cd packages/web-api && pnpm publish --access public",
    "publish:core:beta": "cd packages/core && pnpm publish --access public --tag beta",
    "publish:search:beta": "cd packages/search && pnpm publish --access public --tag beta",
    "publish:navigation:beta": "cd packages/navigation && pnpm publish --access public --tag beta",
    "publish:web-api:beta": "cd packages/web-api && pnpm publish --access public --tag beta",
    "publish:interactive": "bash scripts/publish.sh",
    "version:core": "cd packages/core && pnpm version",
    "version:search": "cd packages/search && pnpm version",
    "version:navigation": "cd packages/navigation && pnpm version",
    "version:web-api": "cd packages/web-api && pnpm version",
    "version:core:beta": "cd packages/core && pnpm version prerelease --preid=beta --no-git-tag-version",
    "version:search:beta": "cd packages/search && pnpm version prerelease --preid=beta --no-git-tag-version",
    "version:navigation:beta": "cd packages/navigation && pnpm version prerelease --preid=beta --no-git-tag-version",
    "version:web-api:beta": "cd packages/web-api && pnpm version prerelease --preid=beta --no-git-tag-version",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "keywords": [
    "monorepo",
    "expo",
    "amap",
    "gaode-map"
  ],
  "author": "TomWq",
  "license": "MIT",
   "repository": {
    "type": "git",
    "url": "https://github.com/TomWq/expo-gaode-map",
    "directory": "packages/core"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "expo": "^54.0.28",
    "expo-module-scripts": "^5.0.8",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "typescript": "^5.9.3"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "expo": "~54.0.28",
    "react": "19.1.0",
    "react-native": "0.81.5"
  }
}
